{
  "vimMode": false,
  "attachmentFolderPath": "files",
  "useMarkdownLinks": false,
  "alwaysUpdateLinks": true,
  "useTab": true,
  "foldIndent": true,
  "smartIndentList": true,
  "showIndentGuide": true,
  "propertiesInDocument": "source",
  "strictLineBreaks": true,
  "livePreview": true,
  "defaultViewMode": "source",
  "pdfExportSettings": {
    "includeName": true,
    "pageSize": "A4",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  },
  "promptDelete": false,
  "userIgnoreFilters": [
    "files/",
    "periodic/daily/",
    "periodic/weekly/",
    "templates/",
    "periodic/templates/"
  ],
  "readableLineLength": true,
  "spellcheck": false,
  "spellcheckDictionary": [
    "хеш-функцию",
    "хеш-функция",
    "хеш-функции",
    "wip"
  ]
}